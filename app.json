{
  "expo": {
    "name": "Stores",
    "slug": "Stores",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.midoriapps.vetchillan",
      "supportsTablet": false
    },
    "android": {
      "package": "com.midoriapps.vetchillan",
      "permissions": ["CAMERA_ROLL", "LOCATION"]
    }
  }
}
